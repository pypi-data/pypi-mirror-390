    "DiscoveryFastDDS": {
      "properties": {
        "type": {
          "const": "fastdds",
          "default": "fastdds",
          "title": "Type",
          "type": "string"
        },
        "with_discovery_server": {
          "default": true,
          "description": "Run the discovery server. It will bind to 0.0.0.0:11811",
          "title": "With Discovery Server",
          "type": "boolean"
        },
        "discovery_server_ip": {
          "default": "0.0.0.0",
          "description": "IP/host/interface of the discovery server. Assumes port of 11811",

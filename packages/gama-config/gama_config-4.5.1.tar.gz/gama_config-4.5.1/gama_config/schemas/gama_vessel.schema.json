    "Camera": {
      "description": "Physical camera housing/mount that can host multiple camera sensors.\n\nRepresents the physical hardware unit including PTZ mechanisms.\nMultiple sensors (color, thermal, etc.) can share the same PTZ control.",
      "properties": {
        "name": {
          "description": "Physical camera housing identifier (e.g., 'bow', 'stern', 'port')",
          "title": "Name",
          "type": "string"
        },
        "with_ptz": {
          "default": false,
          "description": "Whether we should launch a PTZ driver",
          "title": "With Ptz",
          "type": "boolean"
        },
        "sensors": {
          "description": "List of camera sensors mounted in this housing",
          "items": {
            "$ref": "#/$defs/CameraSensor"
          },
          "title": "Sensors",
          "type": "array"
        }
      },
      "required": [
        "name"
      ],
      "title": "Camera",
      "type": "object"
    },
    "CameraSensor": {
      "description": "Individual camera sensor within a physical camera housing.\n\nRepresents a logical camera stream (e.g., color, thermal) that shares\nthe same physical mounting point and PTZ control with other sensors.",
      "properties": {
        "name": {
          "description": "Sensor identifier used in ROS topics and frame IDs. This should be prefixed with the camera housing name",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/CameraSensorType",
          "default": "color",
          "description": "Type of camera sensor/modality"
        },
        "with_camera_info": {
          "default": true,
          "description": "Whether to launch camera_info publisher for this sensor",
          "title": "With Camera Info",
          "type": "boolean"
        },
        "with_pipeline": {
          "default": true,
          "description": "Whether to launch the pipeline",
          "title": "With Pipeline",
          "type": "boolean"
        },
        "with_image_compressor": {
          "default": true,
          "description": "Whether to launch the image compressor",
          "title": "With Image Compressor",
          "type": "boolean"
        }
      },
      "required": [
        "name"
      ],
      "title": "CameraSensor",
      "type": "object"
    },
    "CameraSensorType": {
      "description": "Supported camera sensor types for different sensor modalities.",
      "enum": [
        "color",
        "ir"
      ],
      "title": "CameraSensorType",
      "type": "string"
    },

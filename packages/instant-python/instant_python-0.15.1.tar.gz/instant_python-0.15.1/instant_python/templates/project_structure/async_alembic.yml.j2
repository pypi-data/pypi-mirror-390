- name: alembic
  type: file
  extension: .ini
  template: persistence/async/alembic.ini
- name: migrations
  type: directory
  children:
    - name: versions
      type: directory
    - name: README
      type: file
      extension: .md
      template: persistence/async/README.md
    - name: env
      type: file
      extension: .py
      template: persistence/async/env.py
    - name: models_metadata
      type: file
      extension: .py
      template: persistence/async/models_metadata.py
    - name: script
      type: file
      extension: .py.mako
      template: persistence/async/script.py.mako
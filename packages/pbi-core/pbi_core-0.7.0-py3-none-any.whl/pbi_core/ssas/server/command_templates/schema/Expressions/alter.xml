<Expressions>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="ID" sql:field="ID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ID.Expression" sql:field="ID.Expression" type="xs:string"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="Kind" sql:field="Kind" type="xs:long"/>
        <xs:element minOccurs="0" name="Expression" sql:field="Expression" type="xs:string"/>
        <xs:element minOccurs="0" name="QueryGroupID" sql:field="QueryGroupID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="QueryGroupID.QueryGroup" sql:field="QueryGroupID.QueryGroup" type="xs:string"/>
        <xs:element minOccurs="0" name="ParameterValuesColumnID" sql:field="ParameterValuesColumnID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ParameterValuesColumnID.Table" sql:field="ParameterValuesColumnID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="ParameterValuesColumnID.Column" sql:field="ParameterValuesColumnID.Column" type="xs:string"/>
        <xs:element minOccurs="0" name="MAttributes" sql:field="MAttributes" type="xs:string"/>
        <xs:element minOccurs="0" name="LineageTag" sql:field="LineageTag" type="xs:string"/>
        <xs:element minOccurs="0" name="SourceLineageTag" sql:field="SourceLineageTag" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</Expressions>
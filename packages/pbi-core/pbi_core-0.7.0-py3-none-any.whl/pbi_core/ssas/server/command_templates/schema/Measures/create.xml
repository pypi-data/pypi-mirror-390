<Measures>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="TableID" sql:field="TableID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="TableID.Table" sql:field="TableID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="Expression" sql:field="Expression" type="xs:string"/>
        <xs:element minOccurs="0" name="FormatString" sql:field="FormatString" type="xs:string"/>
        <xs:element minOccurs="0" name="FormatStringDefinition" sql:field="FormatStringDefinition" type="xs:string"/>
        <xs:element minOccurs="0" name="IsHidden" sql:field="IsHidden" type="xs:boolean"/>
        <xs:element minOccurs="0" name="IsSimpleMeasure" sql:field="IsSimpleMeasure" type="xs:boolean"/>
        <xs:element minOccurs="0" name="DisplayFolder" sql:field="DisplayFolder" type="xs:string"/>
        <xs:element minOccurs="0" name="LineageTag" sql:field="LineageTag" type="xs:string"/>
        <xs:element minOccurs="0" name="SourceLineageTag" sql:field="SourceLineageTag" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</Measures>
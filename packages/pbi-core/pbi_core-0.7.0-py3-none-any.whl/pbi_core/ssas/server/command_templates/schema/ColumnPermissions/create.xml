<ColumnPermissions>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="TablePermissionID" sql:field="TablePermissionID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="TablePermissionID.Role" sql:field="TablePermissionID.Role" type="xs:string"/>
        <xs:element minOccurs="0" name="TablePermissionID.TablePermission" sql:field="TablePermissionID.TablePermission" type="xs:string"/>
        <xs:element minOccurs="0" name="ColumnID" sql:field="ColumnID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ColumnID.Table" sql:field="ColumnID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="ColumnID.Column" sql:field="ColumnID.Column" type="xs:string"/>
        <xs:element minOccurs="0" name="MetadataPermission" sql:field="MetadataPermission" type="xs:long"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</ColumnPermissions>
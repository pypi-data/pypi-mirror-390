<Tables>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="ID" sql:field="ID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ID.Table" sql:field="ID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="DataCategory" sql:field="DataCategory" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="IsHidden" sql:field="IsHidden" type="xs:boolean"/>
        <xs:element minOccurs="0" name="ShowAsVariationsOnly" sql:field="ShowAsVariationsOnly" type="xs:boolean"/>
        <xs:element minOccurs="0" name="IsPrivate" sql:field="IsPrivate" type="xs:boolean"/>
        <xs:element minOccurs="0" name="ExcludeFromModelRefresh" sql:field="ExcludeFromModelRefresh" type="xs:boolean"/>
        <xs:element minOccurs="0" name="AlternateSourcePrecedence" sql:field="AlternateSourcePrecedence" type="xs:int"/>
        <xs:element minOccurs="0" name="LineageTag" sql:field="LineageTag" type="xs:string"/>
        <xs:element minOccurs="0" name="SourceLineageTag" sql:field="SourceLineageTag" type="xs:string"/>
        <!-- These two fields, although pulled from the docs, are not supported by the local SSAS instance
        <xs:element minOccurs="0" name="SystemManaged" sql:field="SystemManaged" type="xs:boolean"/>
        <xs:element minOccurs="0" name="ExcludeFromAutomaticAggregations" sql:field="ExcludeFromAutomaticAggregations" type="xs:boolean"/> -->
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</Tables>
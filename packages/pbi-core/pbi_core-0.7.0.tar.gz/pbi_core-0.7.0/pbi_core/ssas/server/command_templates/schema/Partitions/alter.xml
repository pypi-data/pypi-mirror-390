<Partitions>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="ID" sql:field="ID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ID.Table" sql:field="ID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="ID.Partition" sql:field="ID.Partition" type="xs:string"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="DataSourceID" sql:field="DataSourceID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="DataSourceID.DataSource" sql:field="DataSourceID.DataSource" type="xs:string"/>
        <xs:element minOccurs="0" name="QueryDefinition" sql:field="QueryDefinition" type="xs:string"/>
        <xs:element minOccurs="0" name="Type" sql:field="Type" type="xs:long"/>
        <xs:element minOccurs="0" name="Mode" sql:field="Mode" type="xs:long"/>
        <xs:element minOccurs="0" name="DataView" sql:field="DataView" type="xs:long"/>
        <xs:element minOccurs="0" name="QueryGroupID" sql:field="QueryGroupID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="QueryGroupID.QueryGroup" sql:field="QueryGroupID.QueryGroup" type="xs:string"/>
        <xs:element minOccurs="0" name="ExpressionSourceID" sql:field="ExpressionSourceID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ExpressionSourceID.Expression" sql:field="ExpressionSourceID.Expression" type="xs:string"/>
        <xs:element minOccurs="0" name="MAttributes" sql:field="MAttributes" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</Partitions>
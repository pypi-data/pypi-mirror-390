<CalculationItems>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="CalculationGroupID" sql:field="CalculationGroupID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="CalculationGroupID.Table" sql:field="CalculationGroupID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="Expression" sql:field="Expression" type="xs:string"/>
        <xs:element minOccurs="0" name="Ordinal" sql:field="Ordinal" type="xs:int"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</CalculationItems>
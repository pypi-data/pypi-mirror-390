<FormatStringDefinitions>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="ObjectID" sql:field="ObjectID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ObjectID.Table" sql:field="ObjectID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="ObjectID.Measure" sql:field="ObjectID.Measure" type="xs:string"/>
        <xs:element minOccurs="0" name="ObjectID.CalculationItem" sql:field="ObjectID.CalculationItem" type="xs:string"/>
        <xs:element minOccurs="0" name="ObjectType" sql:field="ObjectType" type="xs:int"/>
        <xs:element minOccurs="0" name="Expression" sql:field="Expression" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</FormatStringDefinitions>
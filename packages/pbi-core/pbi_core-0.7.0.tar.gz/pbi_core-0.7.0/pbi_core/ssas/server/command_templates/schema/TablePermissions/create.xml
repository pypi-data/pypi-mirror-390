<TablePermissions>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="RoleID" sql:field="RoleID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="RoleID.Role" sql:field="RoleID.Role" type="xs:string"/>
        <xs:element minOccurs="0" name="TableID" sql:field="TableID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="TableID.Table" sql:field="TableID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="FilterExpression" sql:field="FilterExpression" type="xs:string"/>
        <xs:element minOccurs="0" name="MetadataPermission" sql:field="MetadataPermission" type="xs:long"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</TablePermissions>
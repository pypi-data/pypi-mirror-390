<Levels>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="HierarchyID" sql:field="HierarchyID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="HierarchyID.Table" sql:field="HierarchyID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="HierarchyID.Hierarchy" sql:field="HierarchyID.Hierarchy" type="xs:string"/>
        <xs:element minOccurs="0" name="Ordinal" sql:field="Ordinal" type="xs:int"/>
        <xs:element minOccurs="0" name="Name" sql:field="Name" type="xs:string"/>
        <xs:element minOccurs="0" name="Description" sql:field="Description" type="xs:string"/>
        <xs:element minOccurs="0" name="ColumnID" sql:field="ColumnID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ColumnID.Table" sql:field="ColumnID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="ColumnID.Column" sql:field="ColumnID.Column" type="xs:string"/>
        <xs:element minOccurs="0" name="LineageTag" sql:field="LineageTag" type="xs:string"/>
        <xs:element minOccurs="0" name="SourceLineageTag" sql:field="SourceLineageTag" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
{{rows}}
</Levels>
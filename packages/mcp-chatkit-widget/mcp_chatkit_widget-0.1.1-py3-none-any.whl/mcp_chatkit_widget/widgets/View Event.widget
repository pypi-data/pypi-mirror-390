{
  "version": "1.0",
  "name": "View Event",
  "template": "{\"type\":\"Card\",\"children\":[{\"type\":\"Row\",\"align\":\"stretch\",\"gap\":3,\"children\":[{\"type\":\"Box\",\"width\":5,\"background\":{{ (color) | tojson }},\"radius\":\"full\",\"children\":[]},{\"type\":\"Col\",\"flex\":1,\"gap\":1,\"children\":[{\"type\":\"Row\",\"children\":[{\"type\":\"Text\",\"color\":\"alpha-70\",\"size\":\"sm\",\"value\":{{ ((date.dayName ~ \", \" ~ date.monthName ~ \" \" ~ date.dayNumber)) | tojson }},\"children\":[]},{\"type\":\"Spacer\",\"children\":[]},{\"type\":\"Text\",\"color\":{{ (color) | tojson }},\"size\":\"sm\",\"value\":{{ (time) | tojson }},\"children\":[]}]},{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"md\",\"children\":[]}]}]}]}",

      "time": {
        "type": "string"
      },
      "color": {
        "type": "string"
      },
      "date": {
        "type": "object",
        "properties": {
          "dayName": {
            "type": "string"
          },
          "dayNumber": {
            "type": "string"
          },
          "monthName": {
            "type": "string"
          }
        },
        "required": [
          "dayName",
          "dayNumber",
          "monthName"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "title",
      "time",
      "color",
      "date"
    ],
    "additionalProperties": false
  },

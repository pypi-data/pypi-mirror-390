{
  "version": "1.0",
  "name": "Weather Forecast",
  "template": "{\"type\":\"Card\",\"theme\":\"dark\",\"size\":\"sm\",\"padding\":8,\"background\":{{ (background) | tojson }},\"children\":[{\"type\":\"Col\",\"align\":\"center\",\"gap\":3,\"children\":[{\"type\":\"Image\",\"src\":{{ (conditionImage) | tojson }},\"size\":60,\"children\":[]},{\"type\":\"Row\",\"align\":\"center\",\"gap\":2,\"children\":[{\"type\":\"Title\",\"value\":{{ (lowTemperature) | tojson }},\"size\":\"2xl\",\"weight\":\"normal\",\"color\":\"alpha-70\",\"children\":[]},{\"type\":\"Title\",\"value\":{{ (highTemperature) | tojson }},\"size\":\"2xl\",\"color\":\"emphasis\",\"weight\":\"normal\",\"children\":[]}]},{\"type\":\"Caption\",\"value\":{{ (location) | tojson }},\"color\":\"emphasis\",\"children\":[]},{\"type\":\"Text\",\"value\":{{ (conditionDescription) | tojson }},\"textAlign\":\"center\",\"children\":[]},{\"type\":\"Row\",\"gap\":6,\"children\":[{%- set _c -%}{%-for day in forecast -%},{\"type\":\"Col\",\"align\":\"center\",\"gap\":0,\"children\":[{\"type\":\"Image\",\"src\":{{ (day.conditionImage) | tojson }},\"size\":40,\"children\":[]},{\"type\":\"Text\",\"value\":{{ (day.temperature) | tojson }},\"children\":[]}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "background": {
        "type": "string"
      },
      "conditionImage": {
        "type": "string"
      },
      "lowTemperature": {
        "type": "string"
      },
      "highTemperature": {
        "type": "string"
      },
      "location": {
        "type": "string"
      },
      "conditionDescription": {
        "type": "string"
      },
      "forecast": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "conditionImage": {
              "type": "string"
            },
            "temperature": {
              "type": "string"
            }
          },
          "required": [
            "conditionImage",
            "temperature"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": [
      "background",
      "conditionImage",
      "lowTemperature",
      "highTemperature",
      "location",
      "conditionDescription",
      "forecast"
    ],
    "additionalProperties": false
  },

  "Treatment": {
    "Input": "\nstart(union({id in (30020,30321, 30015,225823,30186,30211,30353,42742,42244,225159,225159,225159); scope = inputevents} *0.5,\n{id in (227531); scope = inputevents} *2.75,\n{id in (30143,225161); scope = inputevents} *3,\n{id in (30009,220862); scope = inputevents} *5,\n{id in (30030,220995,227533); scope = inputevents} *6.66,\n{id in (228341); scope = inputevents} *8)) where #value > 0",
    "Output": "\n{id in (40055, 43175, 40069, 40094, 40715, 40473, 40085, 40057, 40056, 40405, \n 40428, 40096, 40651, 226559, 226560, 227510, 226561, 227489, 226584,\n 226563, 226564, 226565, 226557, 226558,  226627, 226567, 226588, 226606, 226599, 226589, \n 226575, 226576, 226600, 226613, 226597, 226592, 227701, 226598, 226610, 226590, 226626, \n 226593, 226573, 226571, 227511, 226601, 226619, 227488, 226607, 226604, 226608, 226612, \n 226603, 226602, 226631, 226620, 229413, 226632, 226625, 226591, 226617, 226595, 226614, \n 226574, 226628, 226605, 226609, 229414, 226629, 226622, 226623, 226618, 226630, 226570, \n 226624, 226621, 226616, 226569, 226572); scope = outputevents}\n where #value > 0",
    "Dialysis": "{type = event}",
    "OnDialysis": "{id in (225802, 225441); scope = procedureevents}",
    "Norepinephrine": "{Norepinephrine; scope = inputevents; value = amount} where {Norepinephrine; scope = inputevents; value = rateuom} = 'mcg/kg/min'",
    "Epinephrine": "{Epinephrine; scope = inputevents; value = amount} where {Epinephrine; scope = inputevents; value = rateuom} = 'mcg/kg/min'",
    "Phenylephrine": "{Phenylephrine; scope = inputevents; value = amount} where {Phenylephrine; scope = inputevents; value = rateuom} = 'mcg/kg/min'",
    "Dopamine": "{Dopamine; scope = inputevents; value = amount} where {Dopamine; scope = inputevents; value = rateuom} = 'mcg/kg/min'",
    "Vasopressin": "{Vasopressin; scope = inputevents; value = amount} / 60 where {Vasopressin; scope = inputevents; value = rateuom} = 'units/hour'",
    "CardioversionDefibrillation": "{Cardioversion/Defibrillation; scope = procedureevents}",
    "Immunosuppressant": "{name contains /anti\\-thymocyte\\ globulin\\ \\(rabbit\\)|azathioprine|cyclosporine|methylprednisolone|mycophenolate|mycophenolate\\ mofetil|prednisone|ruxolitinib|sirolimus|tacrolimus/i; scope = Medication}",
    "Diuretic": "{name contains /acetazolamide|bumetanide|chlorothiazide|eplerenone|furosemide|hydrochlorothiazide|metolazone|spironolactone|tolvaptan|torsemide/i; scope = Medication}",
    "AntimicrobialTx": "{name contains /abacavir|acyclovir|ambisome|amikacin|amoxicillin|amoxicillin\\-clavulanate|amphotericin\\ b|ampicillin|ampicillin\\-sulbactam|atazanavir|azithromycin|aztreonam|bacitracin|bactrim|caspofungin|cefazolin|cefepime|cefpodoxime\\ proxetil|ceftaroline|ceftolozane\\-tazobactam|ceftriaxone|cephalexin|cidofovir|ciprofloxacin|clarithromycin|clindamycin|clotrimazole|colistin|daptomycin|darunavir|daunorubicin|dicloxacillin|dolutegravir|doxorubicin|doxycycline\\ hyclate|efavirenz|emtricitabine|emtricitabine\\-tenofovir|entecavir|ertapenem|erythromycin|etravirine|fluconazole|flucytosine|foscarnet|ganciclovir|gentamicin|idarubicin|imipenem\\-cilastatin|ketoconazole|lamivudine|lamivudine\\-zidovudine|ledipasvir\\/sofosbuvir|levofloxacin|linezolid|lopinavir\\-ritonavir|meropenem|metronidazole|micafungin|minocycline|moxifloxacin|mupirocin\\ ointment|nafcillin|neomycin|neomycin\\-polymyxin\\-bacitracin|neomycin\\-polymyxin\\-dexameth|nitrofurantoin|nystatin|oseltamivir|penicillin|piperacillin\\-tazobactam|posaconazole|raltegravir|rifampin|rifaximin|ritonavir|sulfamethoxazole\\-trimethoprim|tenofovir|tetracycline|tigecycline|tobramycin|trimethoprim|valacyclovir|valganciclovir|vancomycin|voriconazole/i; scope = Medication}",
    "PositiveCultureSite": "{Culture; scope = Culture; value = spec_type_desc}"

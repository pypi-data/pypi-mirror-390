{
  "openapi": "3.0.3",
  "info": {
    "title": "Todoist API v1",
    "description": "Complete API for Todoist task management system. All requests must be made via HTTPS. Rate limit: varies by endpoint. Documentation: https://developer.todoist.com/api/v1",
    "version": "1.0.0",
    "contact": {
      "name": "Todoist Support",
      "email": "support@todoist.com",
      "url": "https://developer.todoist.com/api/v1"
    }
  },
  "servers": [
    {
      "url": "https://api.todoist.com/api/v1",
      "description": "Todoist API v1 Production Server"
    }
  ],
  "paths": {
    "/user": {
      "get": {
        "summary": "Get Current User",
        "description": "Returns information about the current authenticated user",
        "operationId": "getCurrentUser",
        "tags": ["User"],
        "responses": {
          "200": {
            "description": "Successfully retrieved current user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/tasks": {
      "get": {
        "summary": "List Tasks",
        "description": "Retrieve a list of tasks with cursor-based pagination",
        "operationId": "listTasks",
        "tags": ["Tasks"],
        "parameters": [
          {
            "$ref": "#/components/parameters/Cursor"
          },
          {
            "$ref": "#/components/parameters/Limit"
          },
          {
            "name": "project_id",
            "in": "query",
            "description": "Filter tasks by project ID",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "section_id",
            "in": "query",
            "description": "Filter tasks by section ID",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "label",
            "in": "query",
            "description": "Filter tasks by label",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "filter",
            "in": "query",
            "description": "Filter tasks by filter",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "lang",
            "in": "query",
            "description": "Language for the response",
            "schema": {
              "type": "string",
              "default": "en"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved tasks",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "items": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Task"
                      }
                    },
                    "next_cursor": {
                      "type": "string",
                      "nullable": true,
                      "description": "Cursor for the next page of results"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      },
      "post": {
        "summary": "Create Task",
        "description": "Create a new task",
        "operationId": "createTask",
        "tags": ["Tasks"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateTaskRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Task successfully created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Task"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/tasks/{id}": {
      "get": {
        "summary": "Get Task",
        "description": "Get a specific task by ID",
        "operationId": "getTask",
        "tags": ["Tasks"],
        "parameters": [
          {
            "$ref": "#/components/parameters/TaskId"

  "components": {
    "securitySchemes": {
      "BearerToken": {
        "type": "http",
        "scheme": "bearer",
        "description": "Personal API token obtained from Todoist account settings. Get your token from Settings → Integrations → API token."
      }
    },
    "parameters": {
      "Cursor": {
        "name": "cursor",
        "in": "query",
        "description": "Cursor for pagination (opaque string from previous response)",
        "schema": {
          "type": "string"
        }
      },
      "Limit": {
        "name": "limit",
        "in": "query",
        "description": "Number of items to return per page",
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 200,
          "default": 30
        }
      },
      "TaskId": {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "Task ID",
        "schema": {
          "type": "string"
        }
      },
      "ProjectId": {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "Project ID",
        "schema": {
          "type": "string"
        }
      },
      "LabelId": {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "Label ID",
        "schema": {
          "type": "string"
        }
      }
    },
    "responses": {
      "BadRequest": {
        "description": "Bad request - invalid data provided",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "Unauthorized": {
        "description": "Unauthorized - invalid or missing API token",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "NotFound": {
        "description": "Resource not found",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "schemas": {
      "User": {
        "type": "object",
        "description": "Current authenticated user",
        "properties": {
          "id": {
            "type": "string",
            "description": "User ID",
            "example": "2671355"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "User email address",
            "example": "user@example.com"
          },
          "full_name": {
            "type": "string",
            "description": "User's full name",
            "example": "John Doe"
          },
          "avatar_big": {
            "type": "string",
            "format": "uri",
            "description": "URL to user's avatar (big size)",
            "example": "https://dcff1xvirvpfp.cloudfront.net/5e4c1c9c92aa31.79973322.jpg?size=big"
          },
          "avatar_medium": {
            "type": "string",
            "format": "uri",
            "description": "URL to user's avatar (medium size)",
            "example": "https://dcff1xvirvpfp.cloudfront.net/5e4c1c9c92aa31.79973322.jpg?size=medium"
          },
          "avatar_small": {
            "type": "string",
            "format": "uri",
            "description": "URL to user's avatar (small size)",
            "example": "https://dcff1xvirvpfp.cloudfront.net/5e4c1c9c92aa31.79973322.jpg?size=small"
          },
          "is_premium": {
            "type": "boolean",
            "description": "Whether user has premium subscription",
            "example": false
          },
          "is_biz_admin": {
            "type": "boolean",
            "description": "Whether user is business admin",
            "example": false
          },
          "join_date": {
            "type": "string",
            "format": "date",
            "description": "Date when user joined",
            "example": "2015-05-15"
          }
        },
        "required": ["id", "email", "full_name"]
      },
      "Task": {
        "type": "object",
        "description": "A task in Todoist",
        "properties": {
          "id": {
            "type": "string",
            "description": "Task ID",
            "example": "2995104339"
          },
          "assigner_id": {
            "type": "string",
            "nullable": true,
            "description": "ID of the user who assigned the task",
            "example": "2671355"
          },
          "assignee_id": {
            "type": "string",
            "nullable": true,
            "description": "ID of the user who is assigned to the task",
            "example": "2671362"
          },
          "project_id": {
            "type": "string",
            "description": "ID of the project that contains the task",
            "example": "2203306141"
          },
          "section_id": {
            "type": "string",
            "nullable": true,
            "description": "ID of the section that contains the task",
            "example": "7025"
          },
          "parent_id": {
            "type": "string",
            "nullable": true,
            "description": "ID of the parent task if this is a subtask",
            "example": "2995104589"
          },
          "order": {
            "type": "integer",
            "description": "Order of the task in the project",
            "example": 1
          },
          "content": {
            "type": "string",
            "description": "Content of the task",
            "example": "Buy milk"
          },
          "description": {
            "type": "string",
            "description": "Description of the task",
            "example": "Remember to buy milk from the grocery store"
          },
          "is_completed": {
            "type": "boolean",
            "description": "Whether the task is completed",
            "example": false
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of label IDs associated with the task",
            "example": ["2156144812", "2156144813"]
          },
          "priority": {
            "type": "integer",
            "description": "Priority of the task (1-4, where 1 is the highest priority)",
            "minimum": 1,
            "maximum": 4,
            "example": 1
          },
          "comment_count": {
            "type": "integer",
            "description": "Number of comments on the task",
            "example": 10
          },
          "creator_id": {
            "type": "string",
            "description": "ID of the user who created the task",
            "example": "2671355"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "description": "Date and time when the task was created",
            "example": "2017-09-19T11:25:12.000000Z"
          },
          "due": {
            "type": "object",
            "nullable": true,
            "description": "Due date information",
            "properties": {
              "date": {
                "type": "string",
                "format": "date",
                "description": "Due date",
                "example": "2016-09-01"
              },
              "datetime": {
                "type": "string",
                "format": "date-time",
                "nullable": true,
                "description": "Due date and time",
                "example": "2016-09-01T09:00:00"
              },
              "string": {
                "type": "string",
                "description": "Human-readable due date",
                "example": "tomorrow at 12"
              },
              "timezone": {
                "type": "string",
                "nullable": true,
                "description": "Timezone for the due date",
                "example": "Europe/Moscow"
              }
            }
          },
          "url": {
            "type": "string",
            "format": "uri",
            "description": "URL to view the task in Todoist",
            "example": "https://todoist.com/showTask?id=2995104339"
          },
          "duration": {
            "type": "object",
            "nullable": true,
            "description": "Duration information for the task",
            "properties": {
              "amount": {
                "type": "integer",
                "description": "Duration amount",
                "example": 30
              },
              "unit": {
                "type": "string",
                "enum": ["minute", "day"],
                "description": "Duration unit",
                "example": "minute"
              }
            }
          }
        },
        "required": ["id", "content", "is_completed", "priority", "created_at"]
      },
      "CreateTaskRequest": {
        "type": "object",
        "description": "Request payload for creating a task",
        "properties": {
          "content": {
            "type": "string",
            "description": "Content of the task (required)",
            "example": "Buy milk"
          },
          "description": {
            "type": "string",
            "description": "Description of the task",
            "example": "Remember to buy milk from the grocery store"
          },
          "project_id": {
            "type": "string",
            "description": "ID of the project to add the task to",
            "example": "2203306141"
          },
          "section_id": {
            "type": "string",
            "description": "ID of the section to add the task to",
            "example": "7025"
          },
          "parent_id": {
            "type": "string",
            "description": "ID of the parent task if this is a subtask",
            "example": "2995104589"
          },
          "order": {
            "type": "integer",
            "description": "Order of the task in the project",
            "example": 1
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of label IDs to associate with the task",
            "example": ["2156144812", "2156144813"]
          },
          "priority": {
            "type": "integer",
            "description": "Priority of the task (1-4, where 1 is the highest priority)",
            "minimum": 1,
            "maximum": 4,
            "example": 1
          },
          "due_string": {
            "type": "string",
            "description": "Human-readable due date",
            "example": "tomorrow at 12"
          },
          "due_date": {
            "type": "string",
            "format": "date",
            "description": "Due date",
            "example": "2016-09-01"
          },
          "due_datetime": {
            "type": "string",
            "format": "date-time",
            "description": "Due date and time",
            "example": "2016-09-01T09:00:00"
          },
          "due_lang": {
            "type": "string",
            "description": "Language for the due date",
            "example": "en"
          },
          "assignee_id": {
            "type": "string",
            "description": "ID of the user to assign the task to",
            "example": "2671362"
          }
        },
        "required": ["content"]
      },
      "UpdateTaskRequest": {
        "type": "object",
        "description": "Request payload for updating a task",
        "properties": {
          "content": {
            "type": "string",
            "description": "Content of the task",
            "example": "Buy milk"
          },
          "description": {
            "type": "string",
            "description": "Description of the task",
            "example": "Remember to buy milk from the grocery store"
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of label IDs to associate with the task",
            "example": ["2156144812", "2156144813"]
          },

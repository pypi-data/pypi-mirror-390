{
  "name": "systemd",
  "allowed_commands": [
    "ls",
    "/usr/bin/ls",
    "/bin/ls",
    "cat",
    "/usr/bin/cat",
    "/bin/cat",
    "head",
    "/usr/bin/head",
    "tail",
    "/usr/bin/tail",
    "grep",
    "/usr/bin/grep",
    "/bin/grep",
    "find",
    "/usr/bin/find",
    "df",
    "/usr/bin/df",
    "/bin/df",
    "free",
    "/usr/bin/free",
    "ps",
    "/usr/bin/ps",
    "/bin/ps",
    "uptime",
    "/usr/bin/uptime",
    "stat",
    "/usr/bin/stat",
    "wc",
    "/usr/bin/wc",
    "env",
    "/usr/bin/env",
    "journalctl",
    "/usr/bin/journalctl",
    "/bin/journalctl",
    "systemd-cgls",
    "/usr/bin/systemd-cgls",
    "/bin/systemd-cgls",
    "systemd-cgtop",
    "/usr/bin/systemd-cgtop",
    "/bin/systemd-cgtop"
  ],
  "binds": [
    {
      "source": "/usr",
      "target": "/usr",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/bin",
      "target": "/bin",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/sbin",
      "target": "/sbin",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/lib",
      "target": "/lib",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/lib64",
      "target": "/lib64",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/etc",
      "target": "/etc",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/proc",
      "target": "/proc",
      "read_only": false,
      "create_target": false
    },
    {
      "source": "/sys",
      "target": "/sys",
      "read_only": true,
      "create_target": false
    },
    {
      "source": "/var/log",
      "target": "/var/log",
      "read_only": true,
      "create_target": true
    },
    {
      "source": "/run/systemd",
      "target": "/run/systemd",
      "read_only": true,
      "create_target": true
    },
    {
      "source": "/var/log/journal",
      "target": "/var/log/journal",
      "read_only": true,
      "create_target": true
    },
    {
      "source": "/run/log/journal",
      "target": "/run/log/journal",
      "read_only": true,
      "create_target": true
    }
  ],
  "tmpfs_paths": ["/tmp", "/var/tmp", "/home/systemd"],
  "environment": {
    "HOME": "/home/systemd",
    "PATH": "/usr/bin:/bin:/usr/sbin:/sbin",
    "LANG": "C.UTF-8",
    "LC_ALL": "C.UTF-8"
  },
  "network_isolation": true,
  "additional_args": [
    "--hostname",
    "systemd-sandbox",
    "--chdir",
    "/home/systemd",
    "--dir",
    "/var/cache"
  ]
}

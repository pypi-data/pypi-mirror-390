include README_PYPI.md
include LICENSE
include requirements.txt
include pyproject.toml
# Exclude other .md files from PyPI - only README_PYPI.md is needed
prune docs
exclude *.md
exclude !README_PYPI.md
global-exclude __pycache__
global-exclude *.py[co]
global-exclude venv
global-exclude .git
global-exclude tests
global-exclude examples
global-exclude setup.sh
global-exclude requirements-dev.txt


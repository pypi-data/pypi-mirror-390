			"@container" : "@set"
		},
		"license" : {
			"@id" : "dct:license",
			"@type" : "@id",
			"@context" : {
				"@base" : "http://publications.europa.eu/resource/authority/licence/",
				"@language" : "en",
				"LicenseDocument": "http://purl.org/dc/terms/LicenseDocument",
				"title" : "http://www.w3.org/2004/02/skos/core#prefLabel"

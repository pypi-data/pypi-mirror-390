{
	"@context": {
		"@version" : 1.1,
		"@import" : "https://semiceu.github.io/MLDCAT-AP/releases/2.1.0/context/mldcat-ap.jsonld",
		"dcat" : "http://www.w3.org/ns/dcat#",
		"dct" : "http://purl.org/dc/terms/",

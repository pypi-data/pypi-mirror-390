{
  "mcpServers": {
    "echo": {
      "command": "uvx",
      "args": ["chuk-mcp-echo", "stdio"],
      "env": {}
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "env": {}
    }
  },
  "namespace": "mcp",
  "enable_caching": true,
  "cache_ttl": 300,
  "default_timeout": 10.0,
  "max_retries": 3
}
